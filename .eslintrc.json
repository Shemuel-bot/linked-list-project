{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
       "import/prefer-default-export":"warn",
       "max-classes-per-file": [
        "error",
        { "ignoreExpressions": true, "max": 2 }
    ]
    }
}
